<body>
    <script>
        const Nota1 = Number(prompt("primeira Nota"))
        const Nota2 = Number(prompt("Segunda Nota"))
        const media = (Nota1+Nota2)
        const resutado = (media/2)
        alert(`O Resutado da média é:${resutado}`)
    </script>
</body>
